<IfModule mod_rewrite.c>
   RewriteEngine on
	RewriteBase /
        RewriteRule ^(server-info|server-status) - [L]
        RewriteCond %{REQUEST_FILENAME} !-f
        RewriteCond %{REQUEST_FILENAME} !-d

   	RewriteRule    ^$ app/webroot/    [L]
   	RewriteRule    (.*) app/webroot/$1 [L]
</IfModule>
